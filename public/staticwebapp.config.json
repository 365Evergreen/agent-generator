{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/css/*", "/js/*", "/img/*", "/fonts/*"]
  },
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "/api/*",
      "allowedMethods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"],
      "statusCode": [200, 404]
    }
  ]
}